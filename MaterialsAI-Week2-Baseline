{"metadata":{"kernelspec":{"language":"python","display_name":"Python 3","name":"python3"},"language_info":{"name":"python","version":"3.12.12","mimetype":"text/x-python","codemirror_mode":{"name":"ipython","version":3},"pygments_lexer":"ipython3","nbconvert_exporter":"python","file_extension":".py"},"kaggle":{"accelerator":"nvidiaTeslaT4","dataSources":[{"sourceId":14497385,"sourceType":"datasetVersion","datasetId":9259717}],"dockerImageVersionId":31236,"isInternetEnabled":true,"language":"python","sourceType":"notebook","isGpuEnabled":true}},"nbformat_minor":4,"nbformat":4,"cells":[{"cell_type":"code","source":"%%writefile week1_dataset.py\nimport numpy as np\nimport pandas as pd\nfrom tqdm.notebook import tqdm\n\nclass MaterialsDataset:\n    def __init__(self):\n        self.elements = ['Li','Ni','Co','Mn','O','Si','Ti','Al']\n        self.techniques = ['XRD','TEM','XPS','Raman','PDF','SEM','ICP','NMR']\n    \n    def generate_compositions(self, n_samples=10000):\n        compositions = np.random.dirichlet(np.ones(8), n_samples)\n        battery_mask = np.random.random(n_samples) < 0.7\n        compositions[battery_mask][:, [0,1,2,3,4]] *= 1.5\n        compositions[~battery_mask][:, [4,5,6]] *= 1.5\n        compositions = compositions / compositions.sum(axis=1, keepdims=True)\n        return compositions\n    \n    def assign_techniques(self, composition):\n        ni_co_mn = sum(composition[[1,2,3]])\n        o_content = composition[4]\n        si_content = composition[5]\n        techniques = []\n        if ni_co_mn > 0.4: techniques.extend(['XRD','TEM','XPS'])\n        if o_content > 0.3: techniques.extend(['Raman','PDF'])\n        if si_content > 0.2: techniques.append('SEM')\n        return techniques\n    \n    def create_dataset(self, n_samples=10000):\n        comps = self.generate_compositions(n_samples)\n        dataset = []\n        for i, comp in enumerate(tqdm(comps)):\n            techniques = self.assign_techniques(comp)\n            dataset.append({\n                'id': i, 'composition': str(dict(zip(self.elements, comp))),\n                'formula': ' + '.join([f\"{self.elements[j]}{comp[j]:.0%}\" for j in range(8) if comp[j]>0.05]),\n                'techniques': str(techniques), 'ni_co_mn': float(sum(comp[[1,2,3]])), 'oxygen': float(comp[4])\n            })\n        df = pd.DataFrame(dataset)\n        df.to_csv('week1_materials_dataset.csv', index=False)\n        return df","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-14T14:57:02.478906Z","iopub.execute_input":"2026-01-14T14:57:02.479265Z","iopub.status.idle":"2026-01-14T14:57:02.485970Z","shell.execute_reply.started":"2026-01-14T14:57:02.479235Z","shell.execute_reply":"2026-01-14T14:57:02.485238Z"}},"outputs":[{"name":"stdout","text":"Writing week1_dataset.py\n","output_type":"stream"}],"execution_count":2},{"cell_type":"code","source":"!ls -la *.py  # Must show week1_dataset.py\nimport sys\nprint(\"Python path:\", sys.path[:3])  # Shows import search paths","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-14T14:57:54.485013Z","iopub.execute_input":"2026-01-14T14:57:54.485716Z","iopub.status.idle":"2026-01-14T14:57:54.612543Z","shell.execute_reply.started":"2026-01-14T14:57:54.485689Z","shell.execute_reply":"2026-01-14T14:57:54.611692Z"}},"outputs":[{"name":"stdout","text":"-rw-r--r-- 1 root root 1749 Jan 14 14:57 week1_dataset.py\nPython path: ['/kaggle/working', '/kaggle/lib/kagglegym', '/kaggle/lib']\n","output_type":"stream"}],"execution_count":3},{"cell_type":"code","source":"from week1_dataset import MaterialsDataset  # NOW WORKS!\ndataset = MaterialsDataset()\ndf = dataset.create_dataset(10000)\nprint(\"Week 1 data ready!\")\ndf.head()","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-01-14T14:58:18.757756Z","iopub.execute_input":"2026-01-14T14:58:18.758345Z","iopub.status.idle":"2026-01-14T14:58:19.439163Z","shell.execute_reply.started":"2026-01-14T14:58:18.758302Z","shell.execute_re