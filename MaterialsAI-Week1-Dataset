{"metadata":{"kernelspec":{"language":"python","display_name":"Python 3","name":"python3"},"language_info":{"name":"python","version":"3.12.12","mimetype":"text/x-python","codemirror_mode":{"name":"ipython","version":3},"pygments_lexer":"ipython3","nbconvert_exporter":"python","file_extension":".py"},"kaggle":{"accelerator":"none","dataSources":[],"dockerImageVersionId":31236,"isInternetEnabled":true,"language":"python","sourceType":"notebook","isGpuEnabled":false}},"nbformat_minor":4,"nbformat":4,"cells":[{"cell_type":"markdown","source":"Week 1: Generate 10K Materials Dataset for Characterization","metadata":{}},{"cell_type":"code","source":"import numpy as np\nimport pandas as pd\nimport json\nfrom tqdm import tqdm\n\nclass MaterialsDataset:\n    def __init__(self):\n        self.elements = ['Li','Ni','Co','Mn','O','Si','Ti','Al']\n        self.techniques = ['XRD','TEM','XPS','Raman','PDF','SEM','ICP','NMR']\n    \n    def generate_compositions(self, n_samples=10000):\n        \"\"\"Generate realistic battery/electrolyte compositions\"\"\"\n        compositions = np.random.dirichlet(np.ones(8), n_samples)\n        \n        # Bias toward realistic battery chemistries\n        battery_mask = np.random.random(n_samples) < 0.7\n        compositions[battery_mask][:, [0,1,2,3,4]] *= 1.5  # Li,Ni,Co,Mn,O\n        compositions[~battery_mask][:, [4,5,6]] *= 1.5      # O,Si,Ti\n        \n        compositions = compositions / compositions.sum(axis=1, keepdims=True)\n        return compositions\n    \n    def assign_techniques(self, composition):\n        \"\"\"Rule-based technique recommendation\"\"\"\n        ni_co_mn = sum(composition[[1,2,3]])   # Ni,Co,Mn\n        o_content = composition[4]             # O\n        si_content = composition[5]            # Si\n        \n        techniques = []\n        if ni_co_mn > 0.4: techniques.extend(['XRD','TEM','XPS'])\n        if o_content > 0.3: techniques.extend(['Raman','PDF'])\n        if si_content > 0.2: techniques.append('SEM')\n        \n        return techniques\n    \n    def create_dataset(self, n_samples=10000):\n        print(f\"Generating {n_samples:,} samples...\")\n        comps = self.generate_compositions(n_samples)\n        \n        dataset = []\n        for i, comp in enumerate(tqdm(comps)):\n            techniques = self.assign_techniques(comp)\n            dataset.append({\n                'id': i,\n                'composition': dict(zip(self.elements, comp)),\n                'formula': ' + '.join([f\"{self.elements[j]}{comp[j]:.0%}\" \n                                     for j in range(8) if comp[j]>0.05]),\n                'techniques': techniques,\n                'ni_co_mn': sum(comp[[1,2,3]]),\n                'oxygen': comp[4]\n            })\n        \n        df = pd.DataFrame(dataset)\n        df.to_csv('week1_materials_dataset.csv', index=False)\n        print(f\"Week 1 Complete: {n_samples:,} samples saved\")\n        print(f\"   NiCoMn-rich: {len(df[df.ni_co_mn>0.4]):,} samples\")\n        return df\n\nif __name__ == \"__main__\":\n    dataset = MaterialsDataset()\n    df = dataset.create_dataset(10000)","metadata":{"_uuid":"8f2839f25d086af736a60e9eeb907d3b93b6e0e5","_cell_guid":"b1076dfc-b9ad-4769-8c92-a6c4dae69d19","trusted":true,"execution":{"iopub.status.busy":"2026-01-14T14:28:44.448541Z","iopub.execute_input":"2026-01-14T14:28:44.448797Z","iopub.status.idle":"2026-01-14T14:28:45.936890Z","shell.execute_reply.started":"2026-01-14T14:28:44.448767Z","shell.execute_reply":"2026-01-14T14:28:45.936194Z"}},"outputs":[{"name":"stdout","text":"Generating 10,000 samples...\n","output_type":"stream"},{"name":"stderr","text":"100%|██████████| 10000/10000 [00:00<00:00, 52038.58it/s]\n","output_type":"stream"},{"name":"stdout","text":"Week 1 Complete: 10,000 samples saved\n   NiCoMn-rich: 4,231 samples\n","output_type":"stream"}],"execution_count":1},{"cell_type":"code","source":"!ls -la *.csv","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-